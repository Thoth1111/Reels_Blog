<ul>
    <% @users.each do |user| %>
        <li>
            <a href=<%= user_path(user) %>>
                <img src=<%= user.photo %>>
                <div>
                    <%= user.name %>
                    Number of posts: <%= user.posts_counter %>
                </div>
            </a>
        </li>
    <% end %>
</ul>
    